<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app_web.technology.groceryshopper">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <uses-feature
        android:name="android.hardware.telephony"
        android:required="false" />
    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="false" />

    <application
        android:name=".util.AppController"
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:process=":second"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activitys.DashboardActivity">



        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activitys.Home"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".Activitys.ItemsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activitys.CheckOut"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activitys.Wishlistactivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activitys.Contact"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activitys.AboutUs"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activitys.Login"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activitys.Sign_up"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activitys.DeliveryOption"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activitys.DeliveryDetailAddress"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activitys.OrderReviewPage"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activitys.PayMentActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activitys.FirstMain"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activitys.Items"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activitys.ItemMainCotegory"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activitys.Myorder"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activitys.OfferItem"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activitys.PaymentByCODActivity"
            android:screenOrientation="portrait" />
